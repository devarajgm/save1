<label>
    Name</label>
<input type="text" id="name" />
<label>
    Username</label>
<input type="text" id="user" />
<label>
    Password</label>
<input type="text" id="pass" />
<label>
    Gender</label>
<input type="text" id="gender" />
<input type="button" value="save" id="save" />
<input type="button" value="save1" id="save1" />

    <a href="../Home/About"><button>Next</button></a>

<script src="../../Scripts/jquery-1.6.2.js" type="text/javascript"></script>
<script src="../../Scripts/jquery-1.6.2.min.js" type="text/javascript"></script>
<script type="text/javascript">
    $("#save").click(function () {
        var obj = { "Name": "", "Username": "", "Password": "", "Gender": "" }

        obj.Name = $("#name").val();
        alert(obj.Name);
        obj.Username = $("#user").val();
        alert(obj.Username);
        obj.Password = $("#pass").val();
        alert(obj.Password);
        obj.Gender = $("#gender").val();
        alert(obj.Gender);
        alert(obj);
        $.ajax({
            url: "../Home/save",
            data: JSON.stringify({ stu: obj }),
            type: "POST",
            contentType: "application/json;",
            success: function (data) {
                alert("save success");
            }

        });
    });

</script>
<script type="text/javascript">
    $("#save1").click(function () {

        var li = { "Username": "", "Password": "" }

        var ga = { "Name": "", "Gender": "" }

        li.Username = $("#user").val();
        li.Password = $("#pass").val();
        ga.Name = $("#name").val();
        ga.Gender = $("#gender").val();

        $.ajax({
            url: "../Home/savee",
            data: JSON.stringify({ ee: li, ad: ga }),
            type: "POST",
            contentType: "application/json;",
            success: function (data) {
                alert("save success");
            }
        });
    });

</script>
